{
    "manifest_version": 2,
    "name": "URL Editor",
    "version": "1.0",
    "description": "Edit URL and open in new tab",
    "browser_action": {
        "default_icon": "128.png",
        "default_popup": "popup.html",
        "default_title": "URL Editor"
    },
    "permissions": [
        "tabs"
    ],
    "author": "Becky Peltz"
}
